import java.util.*;
public class Sort_Array {
    public static void main(String[] args) {
        Scanner in =new Scanner(System.in);
        int a[]={1,2,3,4,5};
        for(int i=0;i<a.length;i++){
            if(a[i]>a[i+1]){
                System.out.println("Array is not sorted");
                break;
            }
            else{
                System.out.println("Array is  sorted");
                break;
            }
            
        }
    }
    
}
