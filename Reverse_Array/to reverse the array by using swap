import java.util.*;
public class Reverse_Array {
    public static void main(String[] args) {
        Scanner in=new Scanner(System.in);
        int a[]={1,8,6,4,5};
        int p=0;
        int q=a.length-1;
        while(p<q){
           int temp=a[p];
            a[p]=a[q];
            a[q]=temp;
            p++;
            q--;
        }
        for(int i=0;i<a.length;i++)
        System.out.print(a[i]+" ");
    }
    
}
